            "types": "./dist/controllers/index.d.ts",
            "import": "./dist/controllers/index.js",
            "require": "./dist/controllers/index.cjs"
        },
        "./hooks": {
            "types": "./dist/hooks/index.d.ts",
            "import": "./dist/hooks/index.js",
            "require": "./dist/hooks/index.cjs"
