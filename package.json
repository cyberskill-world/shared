            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./configs": {
            "types": "./dist/configs/index.d.ts",
            "import": "./dist/configs/index.js",
            "require": "./dist/configs/index.cjs"
        },
        "./configs/commitlint": {
            "types": "./dist/configs/commitlint/commitlint.base.d.ts",
            "import": "./dist/configs/commitlint/commitlint.base.js",
            "require": "./dist/configs/commitlint/commitlint.base.cjs"
        },
        "./configs/eslint": {
            "types": "./dist/configs/eslint/eslint.base.d.ts",
            "import": "./dist/configs/eslint/eslint.base.js",
            "require": "./dist/configs/eslint/eslint.base.cjs"
        },
        "./configs/eslint/cra": {
            "types": "./dist/configs/eslint/eslint.cra.d.ts",
            "import": "./dist/configs/eslint/eslint.cra.js",
