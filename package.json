    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "cyberskill": "./dist/commands.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && env NODE_ENV=production tsup",
        "dev": "rimraf dist && tsup --watch",
        "lint-staged": "npx tsx src/commands.ts lint-staged",
        "lint:check": "npx tsx src/commands.ts lint:check",
        "lint:fix": "npx tsx src/commands.ts lint:fix",
        "prepare": "husky",
        "release": "npm run build && dotenv release-it -- --only-version",
        "setup": "npx tsx src/commands.ts setup"
    },
    "dependencies": {
