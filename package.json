{
    "name": "cyberskill",
    "type": "module",
    "version": "2024.11.27",
    "description": "CyberSkill Shared",
    "author": "Stephen Cheng",
    "license": "MIT",
    "homepage": "https://github.com/cyberskill-world/shared#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cyberskill-world/shared.git"
    },
    "bugs": {
        "url": "https://github.com/cyberskill-world/shared/issues"
    },
    "exports": {
        "./configs": {
            "import": "./dist/configs/index.js",
            "require": "./dist/configs/index.cjs"
        },
        "./configs/eslint": {
            "import": "./dist/configs/eslint/eslint.base.js",
            "require": "./dist/configs/eslint/eslint.base.cjs"
        },
        "./configs/eslint/cra": {
            "import": "./dist/configs/eslint/eslint.cra.js",
            "require": "./dist/configs/eslint/eslint.cra.cjs"
        },
        "./configs/eslint/nestjs": {
            "import": "./dist/configs/eslint/eslint.nestjs.js",
            "require": "./dist/configs/eslint/eslint.nestjs.cjs"
        },
        "./configs/eslint/nextjs": {
            "import": "./dist/configs/eslint/eslint.nextjs.js",
            "require": "./dist/configs/eslint/eslint.nextjs.cjs"
        },
        "./configs/eslint/nodejs": {
            "import": "./dist/configs/eslint/eslint.nodejs.js",
            "require": "./dist/configs/eslint/eslint.nodejs.cjs"
        },
        "./configs/eslint/vite": {
            "import": "./dist/configs/eslint/eslint.vite.js",
            "require": "./dist/configs/eslint/eslint.vite.cjs"
        },
        "./configs/commitlint": {
            "import": "./dist/configs/commitlint/commitlint.base.js",
            "require": "./dist/configs/commitlint/commitlint.base.cjs"
        },
        "./constants": {
            "import": "./dist/constants/index.js",
            "require": "./dist/constants/index.cjs"
        },
        "./controllers": {
            "import": "./dist/controllers/index.js",
            "require": "./dist/controllers/index.cjs"
        },
        "./assets/styles/reset.css": "./dist/assets/styles/reset.css",
        "./typescript": {
            "import": "./dist/typescript/index.js",
            "require": "./dist/typescript/index.cjs"
        },
        "./utils": {
            "import": "./dist/utils/index.js",
            "require": "./dist/utils/index.cjs"
        },
        "./commands": {
            "import": "./dist/commands/index.js",
            "require": "./dist/commands/index.cjs"
        }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "cyberskill": "./dist/commands.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npx rimraf dist && env NODE_ENV=production tsup",
        "clean": "npx rimraf node_modules package-lock.json && npm i -f",
        "dev": "npx rimraf dist && tsup --watch",
        "postinstall": "npx simple-git-hooks",
        "lint": "npx tsx src/commands.ts lint",
        "lint:fix": "npx tsx src/commands.ts lint:fix",
        "lint:inspect": "npx @eslint/config-inspector",
        "release": "npm run build && dotenv release-it -- --only-version",
        "reset": "npx tsx src/commands.ts reset",
        "setup": "npx tsx src/commands.ts setup"
    },
    "dependencies": {
        "@antfu/eslint-config": "3.8.0",
        "@commitlint/cli": "19.5.0",
        "@commitlint/config-conventional": "19.5.0",
        "@csmith/release-it-calver-plugin": "2023.12.0",
        "@eslint/config-inspector": "0.5.6",
        "@swc/core": "1.9.2",
        "@types/mongoose-aggregate-paginate-v2": "1.0.12",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "@types/uuid": "10.0.0",
        "@types/yargs": "17.0.33",
        "auto-changelog": "2.5.0",
        "boxen": "8.0.1",
        "chalk": "5.3.0",
        "crypto-js": "4.2.0",
        "dotenv-cli": "7.4.2",
        "eslint": "9.14.0",
        "eslint-config-next": "15.0.3",
        "eslint-plugin-react-hooks": "5.0.0",
        "eslint-plugin-react-refresh": "0.4.14",
        "graphql": "16.9.0",
        "lint-staged": "15.2.10",
        "mongoose": "8.8.1",
        "mongoose-aggregate-paginate-v2": "1.1.2",
        "mongoose-paginate-v2": "1.8.5",
        "node-fetch": "3.3.2",
        "ora": "8.1.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "release-it": "17.10.0",
        "rimraf": "6.0.1",
        "simple-git-hooks": "2.11.1",
        "slugify": "1.6.6",
        "tsup": "8.3.5",
        "tsx": "4.19.2",
        "typescript": "5.6.3",
        "uuid": "11.0.3",
        "yargs": "17.7.2"
    },
    "simple-git-hooks": {
        "pre-commit": "npx tsx src/commands.ts lint-staged",
        "commit-msg": "npx tsx src/commands.ts commitlint"
    }
}